{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_date": "2024-12-30",
  "name": "dark-florist",
  "main": "server.js",
  "assets": {
    "binding": "ASSETS",
    "directory": "dist/",
    "html_handling": "drop-trailing-slash",
    "not_found_handling": "single-page-application",
    "run_worker_first": true
  },
  "workers_dev": true
}
